{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "outDir":"build/ts",
    "sourceMap": true,
    "resolveJsonModule": true,
    "lib": [
      "es6",
      "es7",
      "dom"
    ],
    "noUnusedLocals": true,
  },
  "exclude": [ "build" ],
  "include": [ "typings.d.ts", "src", "scripts" ]
}
